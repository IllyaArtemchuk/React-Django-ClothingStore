{
    test: /\.less$/,
    use: [{
        loader: 'file-loader',
        options: {
            name: 'theme.css'
        }
    },{
        loader: 'less-loader',
        options: { javascriptEnabled: true }// compiles Less to CSS
    }]
},
{
  "extraBabelPlugins": [
    ["import", { "libraryName": "antd", "libraryDirectory": "lib", "style": true }]]
}